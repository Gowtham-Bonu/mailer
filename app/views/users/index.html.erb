<div class="part" ><%= link_to "create a new user", new_user_path %></div>
<table class="table table-dark table-striped">
  <thead>
    <tr>
      <th>name</th>
      <th>email</th>
      <th>profile</th>
      <th>edit</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= image_tag(user.image.url, alt: "image", size: "50x35") if user.image? %></td>
        <td><%= link_to "edit", edit_user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>